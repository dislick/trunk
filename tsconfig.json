{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "jsx": "react",
    "experimentalDecorators": true,
    "allowJs": true,
    "emitDecoratorMetadata": true,
    "strictNullChecks": false,
    "sourceMap": true,
    "typeRoots": [
      "node_modules/@types"
    ],
  },
  "lib": [ "dom", "es2015" ],
  "exclude": [
    "node_modules",
    "build"
  ]
}